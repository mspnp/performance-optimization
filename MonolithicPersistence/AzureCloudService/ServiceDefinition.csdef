<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="AzureCloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WebRole name="WebRole" vmsize="Large">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="SQLDBConnectionString" />
      <Setting name="EventHubName" />
      <Setting name="EventHubNamespace" />
      <Setting name="LogPolicyName" />
      <Setting name="LogPolicyKey" />
    </ConfigurationSettings>
  </WebRole>
</ServiceDefinition>